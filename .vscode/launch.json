{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Ingest Symbol Info",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/ingest_coinapi.py",
      "console": "integratedTerminal",
      "args": ["--symbol-info"]
    },
    {
      "name": "Python: Ingest Orderbook",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/ingest_coinapi.py",
      "console": "integratedTerminal",
      "args": ["--orderbook", "--symbol", "MEXC_SPOT_GIGA_USDT"]
    },
    {
      "name": "Python: Ingest Funding",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/ingest_coinapi.py",
      "console": "integratedTerminal",
      "args": ["--funding"]
    },
    {
      "name": "Python: Ingest OHLCV Auto",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/ingest_coinapi.py",
      "console": "integratedTerminal",
      "args": [
        "--ohlcv",
        "--symbol",
        "MEXC_SPOT_SPX_USDT",
        "--period-id", "5MIN"
      ]
    },
    {
      "name": "Python: Clean OHLCV Data",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/clean_tables.py",
      "console": "integratedTerminal",
      "args": [
        "--symbol",
        "MEXC_SPOT_GIGA_USDT",
        "MEXC_SPOT_SPX_USDT",
        "MEXCFTS_PERP_GIGA_USDT",
        "MEXCFTS_PERP_SPX_USDT"

      ]
    },
    {
      "name": "Python: Clean All Symbols",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/clean_tables.py",
      "console": "integratedTerminal",
      "args": [
        "--symbol",
        "MEXC_SPOT_GIGA_USDT",
        "MEXC_SPOT_SPX_USDT",
        "MEXCFTS_PERP_GIGA_USDT",
        "MEXCFTS_PERP_SPX_USDT"
      ]
    },
    {
      "name": "Python: Compute Mark Price VWAP",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/compute_mark_price_vwap.py",
      "console": "integratedTerminal",
      "args": [
        "--symbol",
      ]
    },
    {
      "name": "Python: Compute Perp Synthetic",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/scripts/compute_perp_synthetic.py",
      "console": "integratedTerminal",
      "args": []
